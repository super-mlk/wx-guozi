.container {
  width: 100%;
  height: 100%;
  overflow: hidden;
}

/*头部搜索  */

.container .headerSearch {
  width: 100%;
  height: 72rpx;
  padding-top: 12rpx;
  margin-bottom: 12rpx;
  border-bottom: 1px solid #e5e5e5;
  position: fixed;
  top: 0rpx;
  left: 0;
  background: #fff;
  z-index: 999;
}

.container .headerSearch .sousuo {
  width: 72%;
  height: 60rpx;
  border-radius: 10px;
  margin-left: 20rpx;
  background: #f5f5f5;
  float: left;
}

.container .headerSearch .sousuo image {
  width: 30rpx;
  height: 30rpx;
  line-height: 30rpx;
  margin-left: 12rpx;
  float: left;
  margin-top: 16rpx;
}

.container .headerSearch .sousuo input {
  width: 80%;
  height: 32rpx;
  float: left;
  font-size: 24rpx;
  color: #ccc;
  margin-left: 20rpx;
  line-height: 32rpx;
  margin-top: 8rpx;
}

.container .quxiao {
  width: 24%;
  height: 60rpx;
  float: right;
  text-align: center;
  line-height: 60rpx;
}

.container .quxiao text {
  font-size: 24rpx;
  letter-spacing: 4rpx;
  color: #ff8824;
}

/*最近购买  */

.container .recent {
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  border-bottom: 6px solid #f4f4f4;
  margin-top: 80rpx;
  position: relative;
  z-index: 888;
}


/*下拉列表  */
.container .recent .select{
  width: 100%;
  height: 168rpx;
  background: #fff;
  position: absolute;
  top: 80rpx;
  left: 0;
}

.container .recent .select .recently{
  width: 97%;
  height: 80rpx;
  margin-left: 20rpx;
  border-bottom: 1px solid #f0f0f0;
  border-top: 1px solid #f0f0f0;
}

.container .recent .select .recently text{
  width: 70%;
  height: 80rpx;
  font-size: 28rpx;
  float: left;
}

.container .recent .select .recently view{
  width: 30%;
  float: right;
  text-align: center;
}

.container .recent .select .recently view image{
  width: 30rpx;
  height: 20rpx;
  vertical-align: middle;
}


.container .recent .select view:last-child{
  border-top: none;
}

.container .recent .recent-title {
  width: 70%;
  height: 80rpx;
  float: left;
  margin-left: 20rpx;
}

.container .recent .recent-title text {
  font-size: 28rpx;
}

.container .recent .recent-title image {
  width: 30rpx;
  height: 30rpx;
  vertical-align: middle;
  margin-left: 20rpx;
}

.container .recent .recent-pic {
  width: 20%;
  height: 80rpx;
  float: right;
  text-align: right;
  margin-right: 20rpx;
  margin-top: 6rpx;
}

.container .recent .recent-pic image {
  width: 30rpx;
  height: 30rpx;
}

/*预览  */
.container .yulan{
  width: 100%;
  height: 200rpx;
  padding-top: 20rpx;
  padding-bottom: 20rpx;
  border-bottom: 1px solid #e8e8e8;
}

.container .yulan .onlineYulan{
  width: 94%;
  height: 200rpx;
  margin: 0 auto;
  display: flex;
  flex-direction: row;
}

.container .yulan .onlineYulan .oy-pic{
  width: 38%;
}

.container .yulan .onlineYulan .oy-pic image{
  width: 100%;
  height: 200rpx;
}

.container .yulan .onlineYulan .oy-content{
  width: 58%;
  margin-left: 2%;
}

.container .yulan .onlineYulan .oy-content text{
  width: 100%;
  height: auto;
  display: block;
  margin-bottom: 12rpx;
}

.container .yulan .onlineYulan .oy-content text:nth-child(1){
  font-size: 36rpx;
  font-weight: bold;
}

.container .yulan .onlineYulan .oy-content text:nth-child(2){
  font-size: 28rpx;
  height: 80rpx;
  overflow: hidden;
  text-overflow: ellipsis;
}

.container .yulan .onlineYulan .oy-content text:nth-child(3){
  font-size: 24rpx;
}


/*每日推荐部分  */

.container .tuijian {
  width: 100%;
  height: 340rpx;
  border-bottom: 6px solid #f4f4f4;
}

.container .tuijian .tuijian-header {
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  font-size: 36rpx;
  font-weight: bold;
  margin-left: 40rpx;
}

.container .tuijian .tuijian-content .tc-left {
  width: 40%;
  height: 200rpx;
  text-align: center;
  float: left;
}

.container .tuijian .tuijian-content .tc-left image {
  width: 90%;
  height: 200rpx;
}

.container .tuijian .tuijian-content .tc-right {
  width: 60%;
  height: 200rpx;
  float: right;
}

.container .tuijian .tuijian-content .tc-right .tcr-title {
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.container .tuijian .tuijian-content .tc-right .tcr-title text {
  color: #484848;
  font-size: 26rpx;
  font-weight: bold;
}

.container .tuijian .tuijian-content .tc-right .tcr-content {
  width: 100%;
  height: 100rpx;
  display: block;
  font-size: 24rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  margin-top: 12rpx;
}

.container .tuijian .tuijian-content .tc-right .tcr-bottom {
  width: 100%;
  height: 40rpx;
}

.container .tuijian .tuijian-content .tc-right .tcr-bottom text {
  font-size: 24rpx;
  width: 40%;
  height: 40rpx;
  background: #f5f3f1;
  text-align: center;
  color: #ff6b00;
  line-height: 40rpx;
  border-radius: 10px;
  font-weight: bold;
  margin-top: 8rpx;
}

.container .tuijian .tuijian-content .tc-right .tcr-bottom text:first-child {
  float: left;
}

.container .tuijian .tuijian-content .tc-right .tcr-bottom text:last-child {
  float: right;
  margin-right: 20rpx;
}

/*列表内容  */

.container .list {
  width: 100%;
  height: auto;
}

.container .list .list-top {
  width: 90%;
  height: 120rpx;
  line-height: 120rpx;
  margin-left: 20rpx;
}

.container .list .list-top text:first-child {
  float: left;
  font-size: 26rpx;
  font-weight: bold;
}

.container .list .list-top text:last-child {
  float: right;
  font-size: 28rpx;
  letter-spacing: 2rpx;
}

.container .list .list-content{
  width: 100%;
  height: 220rpx;
  border-bottom: 1px solid #f0f0f0;
  margin-top: 20rpx;
}

.container .list .list-content .lc-left{
  width: 40%;
  height: 200rpx;
  float: left;
  text-align: center;
}

.container .list .list-content .lc-left image{
  width: 90%;
  height: 200rpx;
}

.container .list .list-content .lc-right{
  width: 60%;
  height: 200rpx;
  float: right;
}

.container .list .list-content .lc-right view:first-child text{
  font-size: 28rpx;
  font-weight: bold;
}
.container .list .list-content .lc-right view:last-child text{
  width: 100%;
  height: 120rpx;
  font-size: 24rpx;
  display: block;
  line-height: 120rpx;
}

/*横排  */
.container .list2 {
  width: 100%;
  height: auto;
}

.container .list2 .list2-top {
  width: 90%;
  height: 120rpx;
  line-height: 120rpx;
  margin-left: 20rpx;
}

.container .list2 .list2-top text:first-child {
  float: left;
  font-size: 36rpx;
  font-weight: bold;
}

.container .list2 .list2-top text:last-child {
  float: right;
  font-size: 28rpx;
  letter-spacing: 2rpx;
}

.container .list2 .h-content{
  width: 100%;
  height: 320rpx;
  border-bottom: 1px solid #ccc;
}

.container .list2 .h-content view{
  width: 33%;
  height: 200rpx;
  float: left;
}

.container .list2 .h-content .pic{
  width: 29%;
  height: 200rpx;
  margin-left: 20rpx;
}

.container .list2 .h-content .three{
  margin-right: 20rpx;
}

.container .list2 .h-content .pic image{
  width: 100%;
  height: 200rpx;
}

.container .list2 .h-content .pic text{
  width: 100%;
  height: 100rpx;
  font-size: 24rpx;
  text-overflow: ellipsis;
  overflow: hidden;
}



